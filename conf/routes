# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET        /                      controllers.HomeController.index
GET        /about                 controllers.HomeController.about
GET        /service               controllers.HomeController.service
GET        /contact               controllers.HomeController.contact
GET        /portfolio-1           controllers.HomeController.portfolio_1
GET        /portfolio-2           controllers.HomeController.portfolio_2
GET        /portfolio-3           controllers.HomeController.portfolio_3
GET        /portfolio-4           controllers.HomeController.portfolio_4
GET        /portfolio-item        controllers.HomeController.portfolio_item
GET        /blog-1                controllers.HomeController.blog_1
GET        /blog-2                controllers.HomeController.blog_2
GET        /blog-post             controllers.HomeController.blog_post
GET        /fullwidth             controllers.HomeController.fullwidth
GET        /sidebar               controllers.HomeController.sidebar
GET        /faq                   controllers.HomeController.faq
GET        /error404              controllers.HomeController.error404
GET        /pricing               controllers.HomeController.pricing

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file          controllers.Assets.versioned(path="/public", file: Asset)
